    <style>
        @keyframes warning{
            0%{background-color: red;}
            100%{background-color: black;}
        }
        body{
            animation-name: warning;
            animation-duration: 0.1s;
            animation-iteration-count: 200;
        }
        .eye{
            animation-name: hello;
            animation-duration: 0.1s;
            animation-timing-function: 200;
        }
        @keyframes hi{ 
            0% {
            height:0px;
            width: 0px;}
            100% {
                height: 100px;
                width: 125px;}

        }
        @keyframes hello{
            0%{background-color: black;}
            25%{background-color: red;}
            75%{background-color:black}
            100%{background-color: red;}
        }
        @keyframes run{
            0% {
            height:0px;
            width: 0px;
            font-size: 0px;
            }
            100% {
                height: 120px;
                width: 320px;
                font-size: 16px;
            }
        }
        .help{
            animation-name: hi;
            animation-duration: 20s;
            height: 100px;
            width: 125px;
            position: fixed;
            top: 450px;
            left: 800px;
        }
        .he{
            animation-name: hi;
            animation-duration: 20s;
            height: 100px;
            width: 125px;
            position: fixed;
            top: 550px;
            left: 800px;
        }
        .morsecode{
            width: 350px;
            padding: 10px;
            border-radius: 50px;
            background-color: #FAF9F6;
            position: fixed;
            top: 400px;
            left: 450px;
            animation-name: run;
            animation-duration: 20s;
        }
    </style>
